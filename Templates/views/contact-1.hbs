            <form class="contact__form"method="POST"action="/contact">
                <div class="form__name">
                    <input type="text" name="Firstname" placeholder="First Name" required>
                    <input type="text" name="Lastname" placeholder="Last Name" required>
                
                </div>
                <input type="email" name="EmailAddress" placeholder="Your Email Address" required>
                <input type="date"name="admit"placeholder="Date Of Admission"required>
                
               <label>List of Courses:</label>
                <select name="program"class="selected">
                    <option value="c">c</option>
                    <option value="c-plus">c++</option>
                    <option value="python">Python</option>
                    <option value="java">java</option>
                    <option value="jdbc">jdbc</option>
